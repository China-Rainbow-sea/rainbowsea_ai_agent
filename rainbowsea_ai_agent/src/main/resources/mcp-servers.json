{
  "mcpServers": {
    "amap-maps": {
      "command": "npx.cmd",
      "args": [
        "-y",
        "@amap/amap-maps-mcp-server"
      ],
      "env": {
        "AMAP_MAPS_API_KEY": "百度地图的你的 API Key"
      }
    },
    "rainbowsea_image_search_mcp_server": {
      "command": "java",
      "args": [
        "-Dspring.ai.mcp.server.stdio=true",
        "-Dspring.main.web-application-type=none",
        "-Dlogging.pattern.console=",
        "-jar",
        "rainbowsea_image_search_mcp_server/target/rainbowsea_image_search_mcp_server-0.0.1-SNAPSHOT.jar"
      ],
      "env": {}
    }
  }
}